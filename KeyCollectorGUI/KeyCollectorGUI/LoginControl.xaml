<UserControl x:Class="KeyCollectorGUI.LoginControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="250" Height="75" HorizontalAlignment="Center" VerticalAlignment="Center">
    <Grid Width="250" Height="75">
        <TextBox Height="23" HorizontalAlignment="Right" Margin="0,12,12,0" Name="username" VerticalAlignment="Top" Width="157" />
        <Label Content="Username" Height="28" HorizontalAlignment="Left" Margin="12,10,0,0" Name="label1" VerticalAlignment="Top" Width="63" />
        <Button Content="Go!" Height="23" HorizontalAlignment="Left" Margin="163,41,0,0" Name="gobutton" VerticalAlignment="Top" Width="75" Click="gobutton_Click" />
    </Grid>
</UserControl>
